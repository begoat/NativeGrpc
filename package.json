{
  "name": "NativeGrpc",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "yarn run clean && yarn run tsc --",
    "clean": "rimraf build",
    "lint": "tslint src/**/*.ts",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:ios": "npm run build && concurrently -r 'npm run watch' 'react-native run-ios'",
    "start:android": "npm run build && concurrently -r 'npm run watch' 'react-native run-android'",
    "tsc": "tsc",
    "test": "jest --coverage",
    "watch": "npm run build -- -w"
  },
  "dependencies": {
    "react": "16.2.0",
    "react-native": "0.53.0"
  },
  "devDependencies": {
    "@types/jest": "^22.1.1",
    "@types/react": "^16.0.36",
    "@types/react-native": "^0.52.8",
    "babel-jest": "22.2.0",
    "babel-preset-react-native": "4.0.0",
    "concurrently": "^3.5.1",
    "jest": "22.2.1",
    "react-test-renderer": "16.2.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  },
  "jest": {
    "preset": "react-native",
    "testRegex": "build/.+\\.(test|spec).js$",
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text-summary",
      "html"
    ],
    "collectCoverageFrom": [
      "build/**/*.js",
      "!build/**/*.spec.js",
      "!build/**/*.index.js"
    ]
  }
}
